name = "book-shelf"
main = "api/index.js"  # <--- updated path here
compatibility_date = "2025-06-01"
compatibility_flags = ["nodejs_compat"]

[assets]
binding = "ASSETS"
not_found_handling = "single-page-application"

[observability]
enabled = true

[[d1_databases]]
